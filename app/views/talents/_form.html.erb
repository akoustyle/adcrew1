<div class="container">
  <%= simple_form_for @talent do |t| %>
    <div class="form-group">
      <%= t.input :category, collection: ["green", "urban", "fashion", "positive"] %>
      <%#= t.select :category, input_html: { class: 'form-control'}  %>
      <%= t.input :name, input_html: { class: 'form-control'} %>
      <%= t.input :about, input_html: { class: 'form-control'} %>
      <%= t.input :title, input_html: { class: 'form-control'} %>
      <%= t.input :insta_link, input_html: { class: 'form-control'} %>
      <%= t.input :youtube_link, input_html: { class: 'form-control'} %>
      <%= t.input :tiktok_link, input_html: { class: 'form-control'} %>
      <%# <%#= t.input :audience_male, input_html: { class: 'form-control'} %>
      <%#= t.input :audience_female, input_html: { class: 'form-control'} %>
      <%= t.input :video, as: :file, input_html: { class: 'form-control'}  %>
      <%= t.input :photos, input_html: { multiple: true, class: 'hidden', id: 'photo-input'}, label_html: { class: 'upload-photo'}, label: 'ðŸ“· Upload a Talent photo' %><%= image_tag "", width: 150, class: "hidden", id: "photo-preview" %>
      <%= t.button :submit, class: 'btn btn-info' %>
    </div>
  <% end %>
</div>
