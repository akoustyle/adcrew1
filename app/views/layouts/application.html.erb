        <link rel="apple-touch-icon" sizes="144x144" href="app/assets/images/logo-tag.jpeg">
        <link rel="apple-touch-icon" sizes="152x152" href="app/assets/images/logo-tag.jpeg">
        <link rel="apple-touch-icon" sizes="180x180" href="app/assets/images/logo-tag.jpeg">
        <link rel="icon"  type="image/jpeg" sizes="32x32" href="app/assets/images/logo-tag.jpeg">
        <link rel="icon"  type="image/jpeg" sizes="96x96" href="app/assets/images/logo-tag.jpeg">
        <link rel="icon"  type="image/jpeg" sizes="16x16" href="app/assets/images/logo-tag.jpeg">
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
        <%= favicon_link_tag asset_path('logo-tag.jpeg') %>
        <%= csrf_meta_tags %>
        <%= csp_meta_tag %>
        <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
        <script src="https://kit.fontawesome.com/b6edebd40a.js" crossorigin="anonymous"></script>
    </head>

    <body id="gradient haut">

